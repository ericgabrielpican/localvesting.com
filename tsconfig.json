{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react-jsx",
    "strict": true,
    "allowJs": false,
    "target": "ESNext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "paths": {
      "~/*": [
        "./*"
      ]
    },
    "types": [
      "nativewind/types"
    ]
  },
  "include": [
    "app",
    "src",
    "**/*.ts",
    "**/*.tsx",
    "expo-router-entry.d.ts"
  ]
}
