{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
      "types": ["nativewind/types"],
    "jsx": "react-jsx",
    "allowJs": true,
    "strict": true,
    "target": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "app",
    "src",
    "expo-router-entry.d.ts"
  ]
}
